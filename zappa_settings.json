{
    "prod": {
        "app_function": "album.app",
        "aws_region": "us-east-1",
        "exclude": [
            "__pycache__",
            ".git/*",
            ".gitignore",
            ".python-version",
            "LICENSE",
            "README.md",
            "requirements.txt",
            "zappa_settings.json",
            ".env"
        ],
        "keep_warm": true,
        "keep_warm_expression": "rate(5 minutes)",
        "memory_size": 128,
        "profile_name": "default",
        "project_name": "album",
        "runtime": "python3.9",
        "s3_bucket": "album-jet-hughes",
        "timeout_seconds": 30,
        "slim_handler":true
    }
}